<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Section5Q9">

    <ImageView
        android:id="@+id/imageView29"
        android:layout_width="261dp"
        android:layout_height="573dp"
        android:scaleType="fitXY"
        android:src="@mipmap/prect3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.924" />

    <ImageView
        android:id="@+id/imageView28"
        android:layout_width="284dp"
        android:layout_height="520dp"
        android:scaleType="fitXY"
        android:src="@mipmap/prect2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.876" />

    <TextView
        android:id="@+id/textView34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/baloo2"
        android:text="Question 9/13"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.049"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.147" />

    <TextView
        android:id="@+id/textView67"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="13dp"
        android:fontFamily="@font/nats_regular"
        android:text="SECTION 5"
        android:textColor="@color/black"
        android:textSize="38sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.462"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/imgback"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:backgroundTint="@color/white"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.044"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.044"
        app:srcCompat="@mipmap/backarrow"
        app:tint="@color/black" />

    <ProgressBar
        android:id="@+id/progressBar"
        android:layout_width="311dp"
        android:layout_height="4dp"
        android:progress="61"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.193"
        style="?android:attr/progressBarStyleHorizontal"
        android:progressDrawable="@drawable/custom_progress_bg1"
        />

    <ImageView
        android:id="@+id/imageView27"
        android:layout_width="313dp"
        android:layout_height="587dp"
        android:scaleType="fitXY"
        android:src="@mipmap/prect1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.489"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.8" />

    <RelativeLayout
        android:layout_width="284dp"
        android:layout_height="573dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.876">

        <RadioGroup
            android:id="@+id/radioGroup"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="30dp">


            <RadioButton
                android:id="@+id/home"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:background="@drawable/radiobtn_off"
                android:button="@null"
                android:fontFamily="@font/nats_regular"
                android:text="Home"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="26sp" />

            <RadioButton
                android:id="@+id/hotel"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="2dp"
                android:layout_marginBottom="10dp"
                android:background="@drawable/radiobtn_off"
                android:button="@null"
                android:fontFamily="@font/nats_regular"
                android:text="Hotel/canteen"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="26sp" />

            <RadioButton
                android:id="@+id/onehome"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:layout_marginTop="2dp"
                android:layout_marginBottom="10dp"
                android:background="@drawable/radiobtn_off"
                android:button="@null"
                android:fontFamily="@font/nats_regular"
                android:text="1 meal home others in hotel/canteen"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="26sp" />

            <RadioButton
                android:id="@+id/hostel"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="2dp"
                android:layout_marginBottom="10dp"
                android:background="@drawable/radiobtn_off"
                android:button="@null"
                android:fontFamily="@font/nats_regular"
                android:text="Hostel"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="26sp" />

            <RadioButton
                android:id="@+id/twohome"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:layout_marginTop="2dp"
                android:layout_marginBottom="10dp"
                android:background="@drawable/radiobtn_off"
                android:button="@null"
                android:fontFamily="@font/nats_regular"
                android:text="2 meal at home others in hotel/canteen"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="26sp" />

            <EditText
                android:id="@+id/oth"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="2dp"
                android:clickable="true"
                android:layout_marginBottom="125dp"
                android:background="@drawable/radiobtn_off"
                android:fontFamily="@font/nats_regular"
                android:hint="Other ___________"
                android:textAlignment="center"
                android:textColorHint="@color/black"
                android:textSize="26sp" />


        </RadioGroup>

        <TextView
            android:id="@+id/textView77"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:fontFamily="@font/baloo2"
            android:text="Where do you have your food daily?"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="16sp" />

        <ImageView
            android:id="@+id/imageView38"
            android:layout_width="16dp"
            android:layout_height="13.99dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="3dp"
            android:layout_marginEnd="214dp"
            android:layout_marginBottom="32dp"
            android:src="@mipmap/blback" />

        <TextView
            android:id="@+id/backbtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="187dp"
            android:layout_marginBottom="12dp"
            android:fontFamily="@font/nats_regular"
            android:text="Back"
            android:textColor="#704CFF"
            android:textSize="26sp"
            android:textStyle="bold" />

        <Button
            android:id="@+id/nextbtn"
            android:layout_width="117dp"
            android:layout_height="50dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="1dp"
            android:layout_marginBottom="18dp"
            android:backgroundTint="#704CFF"
            android:fontFamily="@font/nats_regular"
            android:paddingTop="0dp"
            android:paddingBottom="5dp"
            android:text="Next"
            android:textAlignment="center"
            android:textAllCaps="false"
            android:textSize="26sp" />
    </RelativeLayout>



</androidx.constraintlayout.widget.ConstraintLayout>